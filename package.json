{
  "name": "autocom",
  "version": "1.0.0",
  "description": "autocom",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "watch": "nodemon --exec electron ."
  },
  "build": {
    "appId": "com.electron.autocom",
    "productName": "AutoCom",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "repository": "https://github.com/tienquocho2604/AutoCom",
  "publish": {
    "provider": "github",
    "releaseType": "release",
    "forceDev": true
  },
  "author": "TienQuocHo",
  "license": "",
  "devDependencies": {
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^6.1.4",
    "md5": "^2.3.0",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.0.1",
    "serialport": "^10.3.0",
    "serialport-gsm": "^4.0.1",
    "socket.io": "^4.7.2"
  },
  "engines": {
    "node": "20.9.0",
    "npm": "9.5.1"
  }
}
